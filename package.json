{
  "name": "@0dep/piso",
  "version": "3.0.1",
  "description": "ISO 8601 interval, date, and duration parser",
  "sideEffects": false,
  "type": "module",
  "module": "./src/index.js",
  "main": "./lib/index.cjs",
  "types": "./types/index.d.ts",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "require": "./lib/index.cjs",
      "import": "./src/index.js"
    }
  },
  "scripts": {
    "test": "mocha",
    "posttest": "npm run lint && npm run dist && npm run test:md",
    "lint": "eslint . --cache && prettier . --check --cache",
    "prepublishOnly": "npm run dist",
    "cov:html": "c8 -n src -r html -r text mocha",
    "test:lcov": "c8 -n src -r lcov -r text mocha",
    "test:md": "texample",
    "dist": "rollup -c && dts-buddy"
  },
  "engines": {
    "node": ">=14"
  },
  "keywords": [
    "ISO8601",
    "interval",
    "duration",
    "date",
    "week",
    "weeknumber",
    "ordinal",
    "to milliseconds",
    "iso",
    "8601",
    "parse",
    "toISOString"
  ],
  "author": {
    "name": "Zerodep AB",
    "url": "https://github.com/zerodep"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zerodep/piso.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@types/chai": "^5.0.0",
    "@types/mocha": "^10.0.3",
    "@types/node": "^18.19.7",
    "c8": "^10.1.3",
    "chai": "^5.0.0",
    "chronokinesis": "^6.0.0",
    "dts-buddy": "^0.6.0",
    "eslint": "^9.20.1",
    "mocha": "^11.0.1",
    "prettier": "^3.5.0",
    "rollup": "^4.10.0",
    "texample": "^0.0.8",
    "ts-node": "^10.9.1"
  },
  "files": [
    "lib/",
    "src/",
    "types/index.d.ts*"
  ]
}
